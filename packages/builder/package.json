{
  "name": "@urlspec/builder",
  "version": "0.1.0",
  "description": "Programmatic API to build URLSpec files",
  "repository": {
    "type": "git",
    "url": "https://github.com/daangn/urlspec.git",
    "directory": "packages/builder"
  },
  "license": "MIT",
  "author": "Danggeun Market Inc.",
  "keywords": [
    "urlspec",
    "url",
    "builder",
    "api",
    "type-safe",
    "url-builder"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest --run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@urlspec/language": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "24",
    "tsdown": "^0.20.0-beta.3",
    "vitest": "^3.0.5"
  }
}
