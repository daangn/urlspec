
param sortOrder = "recent" | "popular";
param jobStatus = "active" | "closed";

global {
  utm_source?: string;
  referrer?: string;
}

page list = /jobs {
  category?: string;
  sort_order: sortOrder;
}

page detail = /jobs/:job_id {
  job_id: string;
  preview?: "true" | "false";
  status?: jobStatus;
}
